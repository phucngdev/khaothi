{
  "name": "enosta-react-restful-ts-base",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "codegen": "eval $(grep '^VITE_API_SCHEMAS_URL' .env) && openapi-rq -i $VITE_API_SCHEMAS_URL -o src/api -c axios --indent 2 && eslint --ext .tsx,.ts,.js,.jsx src/api --fix",
    "check-types": "tsc --noEmit --pretty",
    "check-types:watch": "tsc --noEmit --watch",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src/ --fix",
    "lint-staged": "lint-staged",
    "build": "set DISABLE_ESLINT_PLUGIN=true && vite build",
    "build:analyze": "DISABLE_ESLINT_PLUGIN=true ANALYZE=true vite build",
    "serve": "vite preview",
    "postinstall": "husky install",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "test:units": "vitest run tests/unit",
    "test:integrations": "vitest run tests/integrations",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "sh .storybook/chromatic.sh"
  },
